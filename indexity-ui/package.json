{
  "name": "indexity-ui",
  "version": "1.4.0",
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --output-path=dist",
    "build:prod": "ng build --prod --output-path=dist",
    "build:test": "ng build --prod --output-path=dist-test --configuration=test",
    "build:prod:stats": "ng build --prod --output-path=dist --stats-json",
    "test": "ng test indexity-ui --watch=false --browsers=ChromeHeadless,FirefoxHeadless",
    "test:dev": "ng test indexity-ui --watch=true --browsers=Chrome,Firefox",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint --fix . --ext .ts,.tsx",
    "lib:build": "ng build indexity-annotations --prod",
    "lib:test": "ng test indexity-annotations --watch=false --browsers=ChromeHeadless,FirefoxHeadless",
    "lib:test:dev": "ng test indexity-annotations --watch=true --browsers=Chrome,Firefox",
    "lib:lint": "ng lint indexity-annotations",
    "e2e": "cypress open",
    "print-env": "print-env",
    "cypress": "npm run build:test && npm run cypress:ci",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "CYPRESS_baseUrl=http://0.0.0.0:8000 run-p --race start:ci cypress:run",
    "cy:verify": "cypress verify",
    "start:ci": "cp ./dist-test/index.html ./dist-test/404.html && http-server ./dist-test -p 8000 -c-1",
    "doc": "compodoc -p tsconfig.json --theme stripe",
    "bundle-report": "npm run build:prod:stats && npx webpack-bundle-analyzer dist/stats-es2015.json",
    "ngcc": "ngcc",
    "postinstall": "npm run ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@auth0/angular-jwt": "^4.0.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@types/lodash": "^4.14.139",
    "angular-resizable-element": "^3.3.0",
    "bootstrap": "^4.4.1",
    "fs-extra": "^8.1.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.15",
    "ngx-autosize": "^1.6.3",
    "ngx-color-picker": "^8.1.0",
    "ngx-infinite-scroll": "^9.0.0",
    "ngx-perfect-scrollbar": "^9.0.0",
    "ngx-socket-io": "^3.0.1",
    "path": "^0.12.7",
    "rxjs": "^6.5.3",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "css-element-queries": "^1.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@bahmutov/print-env": "^1.1.0",
    "@ngrx/schematics": "^9.2.0",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.29",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chance": "^1.1.4",
    "codelyzer": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsdoc": "^3.4.3",
    "karma": "~4.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^2.0.2",
    "rxjs-tslint-rules": "^4.27.1",
    "ts-node": "~8.5.4",
    "typedoc": "^0.15.4",
    "typescript": "3.7.5"
  }
}
