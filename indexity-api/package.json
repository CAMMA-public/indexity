{
  "name": "indexity-api",
  "version": "1.4.0",
  "description": "",
  "author": "IRCAD IHU RD Team",
  "scripts": {
    "build": "rimraf dist && gulp build",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint --fix . --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests",
    "migration:create": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm -f src/ormconfig.ts migration:create -d src/migrations",
    "migration:generate": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm -f src/ormconfig.ts migration:generate -d src/migrations",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm -f src/ormconfig.ts migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm -f src/ormconfig.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.8",
